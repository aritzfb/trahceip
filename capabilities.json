{
    "dataRoles": [
        {
            "displayName": "Category Data",
            "name": "category",
            "kind": "Grouping"
            
        },
        {
            "displayName": "Measure Data",
            "name": "measure",
            "kind": "Measure",
            "requiredTypes": [
                {
                    "integer": true
                },
                {
                    "numeric": true
                }
            ]
        }
        ,
        {
            "displayName": "Tooltips",
            "name": "Tooltips",
            "kind": "Measure"
        }
    ]
    ,
    "sorting": {
        "default":{}
    },
    "supportsLandingPage": true,
    "objects": {
        "dataLabels": {
            "displayName": "Trahceip Labels"
            
            , "properties": {
                "fontSize":{
                    "displayName": "Font size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }, 
                "fontColor": {
                    "displayName": "Font color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }, 
                "cracyLabels": {
                    "displayName": "Cracy labels",
                    "type": {
                        "bool": true
                    }
                }, 
                "show": {
                    "type": {
                        "bool": true
                    }
                }
                
            }
        },
        "dataPoint": {
            "displayName": "Data colors",
            "properties": {
                "defaultColor": {
                    "displayName": "Default color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "showAllDataPoints": {
                    "displayName": "Show all",
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "displayName": "Fill",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fillRule": {
                    "displayName": "Color saturation",
                    "type": {
                        "fill": {}
                    }
                },
                 "fontSize": {
                    "displayName": "Text Size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [{
                "category": {"max": 1}
                ,"measure": {"max": 1} 
            }],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure"
                            }
                        }
                    ]
                }
            }
        }
    ]
    ,
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": [
            "Tooltips"
        ]
    }
}
